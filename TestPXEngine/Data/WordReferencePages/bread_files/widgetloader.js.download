function loadWidget(e,t){let i=document.getElementById(t);i.style.position="relative",i.style.minHeight="50px",i.innerHTML="<div class='widgetloader'><div class='lds-ellipsis' style='opacity: 1'><div></div><div></div><div></div><div></div></div></div><div class='widgetcontent' style='height:100%;width:100%;border:none;opacity:0;transition:all 0.5s ease-out 0.3s;'></div>",function(){let t=new XMLHttpRequest;t.onreadystatechange=function(){if(t.readyState===XMLHttpRequest.DONE&&200===t.status){let e=t.responseText;i.getElementsByClassName("widgetloader")[0].remove(),i.getElementsByClassName("widgetcontent")[0].innerHTML=e,i.style.minHeight="initial",i.getElementsByClassName("widgetcontent")[0].style.opacity="1"}},t.open("GET",e),t.send()}()}